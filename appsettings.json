{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ConnectionString": "Data Source=Server Name;Initial Catalog=GrowGreen;Integrated Security=True;TrustServerCertificate=True;"
  },
  "Cloudinary": {
    "CloudName": "CloudName",
    "ApiKey": "Api Key",
    "ApiSecret": "Secret Key"
  },
  "JwtSettings": {
    "Key": "Secret key 32 bits",
    "Issuer": "http://localhost:5045/",
    "Audience": "http://localhost:3000/",
    "TokenExpiryInMinutes": 60
  }
}
